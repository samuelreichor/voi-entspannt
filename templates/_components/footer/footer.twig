<footer class="footer">
    <div class="container">
        <div class="row">
            <div class="footer-col">
                <h4>Wichtige Links</h4>
                <ul>
                    {% for link in footer.navLink.all() %}
                    {% set destination = link.linkDestination.one() %}
                    <li>
                        <a href="{{destination.url}}">{{link.linkText}}</a>
                    </li>
                    {% endfor %}
                </ul>
            </div>
            <div class="footer-col">
                <h4>Langweilige Links</h4>
                <ul>
                    {% for linkFooter in footer.navLinkFooter.all() %}
                    {% set destinationFooter = linkFooter.linkDestinationFooter.one() %}
                    <li>
                        <a href="{{destinationFooter.url}}">{{linkFooter.linkTextFooter}}</a>
                    </li>
                    {% endfor %}
                </ul>
            </div>
            
